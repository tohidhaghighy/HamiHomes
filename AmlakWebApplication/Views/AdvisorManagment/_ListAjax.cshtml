@model List<DomainLayer.Adviser>
@{
    if (Model.Count() > 0)
    {
        foreach (var item in Model)
        {
               <tr>
                   <td class="text-center" style="padding:50px;">@item.Name</td>
                   <td class="text-center" style="padding:50px;">
                       @item.Family
                   </td>
                   <td class="text-center" style="padding:50px;">
                       @item.Mobile
                   </td>
                   <td class="text-center" style="padding:50px;">
                       @item.Username
                   </td>
                   <td class="text-center" style="padding:50px;">
                       @{
                           if (item.Active == true)
                           {
                               <input type="checkbox" checked="checked" onchange="Changecheckuser(@item.Id)" />
                           }
                           else
                           {
                               <input type="checkbox" onchange="Changecheckuser(@item.Id)" />
                           }
                       }

                   </td>
                   <td class="text-center" style="padding:50px;"><p data-placement="top" data-toggle="tooltip" title="حذف"><a class="btn btn-danger btn-xs" onclick="deleteitem(@item.Id)"><i class="ion ion-ios-trash"></i></a></p></td>
               </tr>
        }

    }

}


