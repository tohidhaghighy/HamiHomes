@model List<DomainLayer.Customer>
@{
    if (Model.Count() > 0)
    {
        foreach (var item in Model)
        {
            <tr>
                <td class="text-center" style="padding:20px;">
                    @item.Code
                </td>
                <td class="text-center" style="padding:20px;">
                    @item.Name @item.Family
                </td>

                <td class="text-center" style="padding:20px;"><p data-placement="top" data-toggle="tooltip" title="انتخاب"><a class="btn btn-success btn-xs" onclick="select(@item.Id)">انتخاب</a></p></td>
            </tr>

        }

    }

}


