@model List<DomainLayer.City>
@{
    if (Model.Count() > 0)
    {
        foreach (var item in Model)
        {
    <tr>
        <td class="text-center" style="padding:10px;">@item.Code</td>
        <td class="text-center" style="padding:10px;">@item.Name</td>
        <td class="text-center" style="padding:10px;">
            @item.Region.Name
        </td>
        <td class="text-center" style="padding:10px;">
            @item.CityPhoneCode
        </td>
        <td class="text-center" style="padding:10px;"><p data-placement="top" data-toggle="tooltip" title="افزودن ناحیه"><a class="btn btn-danger btn-xs" onclick="Nahieitem(@item.Id)">افزودن منطقه</a></p></td>
        <td class="text-center" style="padding:10px;"><p data-placement="top" data-toggle="tooltip" title="حذف"><a class="btn btn-danger btn-xs" onclick="deleteitem(@item.Id)"><i class="ion ion-ios-trash"></i></a></p></td>
    </tr>
        }

    }

}


