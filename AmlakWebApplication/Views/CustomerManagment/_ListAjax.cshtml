@model List<DomainLayer.Customer>
@{
    if (Model.Count() > 0)
    {
        foreach (var item in Model)
        {
    <tr>
        <td class="text-center" style="padding:20px;">@item.Code</td>
        <td class="text-center" style="padding:20px;">@item.Name</td>
        <td class="text-center" style="padding:20px;">
            @item.Family
        </td>
        <td class="text-center" style="padding:20px;">
            @item.Number
        </td>
        <td class="text-center" style="padding:20px;">
            @item.Shenasname
        </td>
        <td class="text-center" style="padding:20px;">
            @item.Fathername
        </td>
        <td class="text-center" style="padding:20px;">
            @item.Birthdate
        </td>
        <td class="text-center" style="padding:20px;">
            @item.Address
        </td>
        <td class="text-center" style="padding:20px;"><p data-placement="top" data-toggle="tooltip" title="حذف"><a class="btn btn-success btn-xs" onclick="update(@item.Id)">ویرایش</a></p></td>
        <td class="text-center" style="padding:20px;"><p data-placement="top" data-toggle="tooltip" title="حذف"><a class="btn btn-danger btn-xs" onclick="deleteitem(@item.Id)"><i class="ion ion-ios-trash"></i></a></p></td>
    </tr>

        }

    }

}


